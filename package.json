{
  "name": "electron-vue-demo",
  "version": "1.0.0",
  "description": "A electron vue demo",
  "main": "src/main/index.js",
  "author": "",
  "private": true,
  "license": "ISC",
  "scripts": {
    "start": "node ./build/run.dev.js",
    "dev": "node ./build/run.dev.js",
    "build": "node ./build/run.build.js",
    "build-test": "node ./build/run.build-test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "electron": "8.0.0",
    "electron-builder": "^22.3.2",
    "file-loader": "^5.0.2",
    "hash-sum": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-watch": "^0.6.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.4",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "axios": "^0.19.2",
    "electron-log": "^4.0.6",
    "electron-updater": "^4.2.2",
    "iview": "^3.5.4",
    "js-cookie": "^2.2.1",
    "qs": "^6.9.1",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "build": {
    "productName": "eletron-vue",
    "appId": "com.xxx.electron-vue",
    "copyright": "www.xxx.com all rights reserved",
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    },
    "files": [
      "src/**/*",
      "dist/webpack/**/*",
      "package.json",
      "*.yml"
    ],
    "directories": {
      "output": "dist/electron"
    },
    "win": {
      "icon": "build/icons/aims.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./build/icons/installer.ico",
      "uninstallerIcon": "./build/icons/uninstaller.ico",
      "installerHeaderIcon": "./build/icons/installer.header.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "electron-vue",
      "include": "build/script/installer.nsh"
    },
    "mac": {
      "icon": "build/icons/app.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "icon": "build/icons"
    },
    "publish": {
      "provider": "generic",
      "url": "http://localhost/release/electron-vue/",
      "updaterCacheDirName": "electron-updater"
    },
    "webpack": {
      "devServer": {
        "port": 8082,
        "contentBase": "dist/webpack"
      }
    }
  }
}
